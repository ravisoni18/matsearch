{
    "welcomeFile": "/webapp/index.html",
    "authenticationMethod": "none",
    "routes": [
        {
            "source": "^/sap/(.*)$",
            "destination": "sap-backend",
            "authenticationType": "none"
        },
        {
            "source": "^(.*)$",
            "target": "$1",
            "service": "html5-apps-repo-rt",
            "authenticationType": "none"
        }
    ]
}
