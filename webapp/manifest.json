{
    "sap.app": {
        "id": "knmtapp",
        "type": "application",
        "title": "KNMT Request Management",
        "description": "KNMT Request Management Application",
        "applicationVersion": {
            "version": "1.0.0"
        },
        "sourceTemplate": {
            "id": "@sap/ux-app-migrator:freestyle",
            "version": "1.18.7",
            "toolsId": "15da5ae2-a452-4da7-bb23-a2ab612cad80"
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "rootView": {
            "viewName": "knmtapp.view.App",
            "type": "XML",
            "id": "app"
        },
        "dependencies": {
            "minUI5Version": "1.60",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
                "sap.ui.layout": {}
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "css/style.css"
                }
            ]
        },
        "routing": {
            "config": {
                "routerClass": "sap.m.routing.Router",
                "viewType": "XML",
                "viewPath": "knmtapp.view",
                "controlId": "app",
                "controlAggregation": "pages",
                "async": true
            },
            "routes": [
                {
                    "pattern": "",
                    "name": "login",
                    "target": "login"
                },
                {
                    "pattern": "login",
                    "name": "login",
                    "target": "login"
                },
                {
                    "pattern": "register",
                    "name": "register",
                    "target": "register"
                },
                {
                    "pattern": "list",
                    "name": "list",
                    "target": "list"
                },
                {
                    "pattern": "create",
                    "name": "create",
                    "target": "create"
                },
                {
                    "pattern": "detail/{kunnr}/{vkorg}/{kdmat}",
                    "name": "detail",
                    "target": "detail"
                }
            ],
            "targets": {
                "login": {
                    "viewName": "Login",
                    "viewLevel": 0
                },
                "register": {
                    "viewName": "Register",
                    "viewLevel": 0
                },
                "list": {
                    "viewName": "List",
                    "viewLevel": 1
                },
                "create": {
                    "viewName": "Create",
                    "viewLevel": 2
                },
                "detail": {
                    "viewName": "Detail",
                    "viewLevel": 2
                }
            }
        }
    }
}